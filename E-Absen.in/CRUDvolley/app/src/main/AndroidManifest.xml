<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.febriputratrisbudiono.crudvolley">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:icon="@drawable/e_absenin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/e_absenin"
        android:supportsRtl="true"
        android:theme="@style/Theme.CRUDVolley"
        android:usesCleartextTraffic="true">
        <activity android:name=".Profile.Profile"></activity>
        <activity android:name=".Login.Login"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard.Dashboard" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Absenku.Absenku" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Absen.Absen" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Anggota.Lihat_data" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Anggota.Tampil_data" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".MainActivity" />
    </application>

</manifest>